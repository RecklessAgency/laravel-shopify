{
    "name": "reckless/laravel-shopify",
    "description": "Laravel shopify package",
    "license": "MIT",
    "authors": [
        {
            "name": "michael osei ntow",
            "email": "oseintow@gmail.com"
        },
        {
            "name": "Reckless",
            "email": "dev@reckless.agency"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Reckless\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reckless\\Shopify\\ShopifyServiceProvider"
            ],
            "aliases": {
                "Shopify": "Reckless\\Shopify\\Facades\\Shopify"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
